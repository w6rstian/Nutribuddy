<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Nutribuddy.UI.WPF.ViewModel"
                    xmlns:view="clr-namespace:Nutribuddy.UI.WPF.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProfileVM}">
        <view:Profile />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MealsVM}">
        <view:Meals />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DishEatVM}">
        <view:DishEatConfirmation />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductEatVM}">
        <view:ProductEatConfirmation />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:DishesVM}">
        <view:Dishes />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductsVM}">
        <view:Products />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsVM}">
        <view:Settings />
    </DataTemplate>

</ResourceDictionary>